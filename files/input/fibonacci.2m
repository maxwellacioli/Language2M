fibonacci( inteiro limite ) inteiro {
	inteiro contador;
	inteiro fib1;
	inteiro fib2;
	inteiro fib3;
	
	fib1 = 1;
	fib2 = 1;
	
	se 	(limite == 0) {
		imprima ("0");
	};
	
	enquanto (contador < limite) {
		se (contador < 2) {
			imprima ("1 ");
	    } entao {
		fib3 = fib1 + fib2;
		fib1 = fib2;
		fib2 = fib3;
		imprima (fib3 ++ " ");
	    };
	contador = contador + 1;
	};
};

principal () inteiro {
	inteiro limite;
	leia ( inteiro , limite);
	fibonacci(limit);
};
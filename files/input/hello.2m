fib (inteiro x) inteiro {
    inteiro aux1, aux2;
    inteiro result;

    se((x == 1) ou (x == 2)) {
        result = 1;
    } senao {
        aux1 = fib(x - 1);
        aux2 = fib(x - 2);
        result = aux1 + aux2;
    };

    retorne result;
}

principal () inteiro {
    inteiro x, y, z;
    x = 5;

    z = fib(10);

    imprima(z);
}
divisao (inteiro a, inteiro b) inteiro {
    inteiro x;

    x = a / b;

    retorne x;
}

soma(inteiro a, inteiro b) inteiro {
    inteiro c;

    c = a + b;

    retorne c;
}

principal () inteiro {
    inteiro x, y, z;
    x = 20;
    y = 10;

    z = soma(x, y);
    z = divisao(z, 2);

    imprima(z);
}
divisao (inteiro a, inteiro b) inteiro {
    inteiro x;

    x = a / b;

    retorne x;
}

soma(inteiro a, inteiro b) inteiro {
    inteiro c;

    c = a + b;

    retorne c;
}

principal () inteiro {
    inteiro x, y, z;
    x = 20;
    y = 10;

    z = soma(x, y);

    imprima("A soma de x e y é: " ++ z ++ "\n");

    z = divisao(x, y);

    imprima("A divisão de x por y é: " ++ z ++ "\n");

}